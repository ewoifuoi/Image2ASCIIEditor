<Application
    x:Class="Image2ASCIIEditor.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Image2ASCIIEditor" xmlns:media="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    RequestedTheme="Dark">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
            <media:AcrylicBrush x:Key="CustomAcrylicInAppLuminosity"
            TintOpacity="0" TintLuminosityOpacity="0" TintColor="Transparent" FallbackColor="Transparent" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
